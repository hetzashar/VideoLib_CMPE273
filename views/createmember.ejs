        	<div id="templatemo_header">
				<div id="templatemo_logo_section">            
		        	<h1><a href="index.html">Video Library</a></h1>           
					<h2>Store for all movies: Comedy, Thriller, Action, Romantic, Documentary</h2>
				</div>
				<div id="templatemo_user_section">            
		        	Welcome User!!
				</div>
         	</div><!-- end of headder -->
               <div id="templatemo_menu_panel"> 
				<div id="templatemo_menu_section">
					
						<ul>
						  <li><a href="createmember.html">Create member    </a></li>
						  <li><a href="listmember.html">List Members   </a></li>
						  <li><a href="createmovie.html">Create Movie   </a></li>
						  <li><a href="listmovie.html">List Movies   </a></li>	
						  <li><a href="issuemovie.html">Issue Movie   </a></li>
						  <li><a href="submitmovie.html">Submit Movie  </a></li>  
						  <li><a href="generatebill.html">Generate Bill  </a></li> 
						</ul> 
						
					</div>
             </div> <!-- end of menu -->
		</div><!-- end of container-->
        
	</div><!-- end of templatemo_background_section_top-->
    
    <div id="templatemo_background_section_middle">
    
    	<div class="templatemo_container">
        
        	<div id="templatemo_left_section">
            	
                <div class="templatemo_post">
                
                	<div class="templatemo_post_top">
                    	<h1>Create Member</h1>
                    </div>
                    <div class="templatemo_post_mid">
                    	<table border="0">
							<tbody>

							<tr>
							<td><label for="name">Member type: </label> </td>
							<td><input type="radio" name="memType" value="S">Simple
								<input type="radio" name="memType" value="P">Premium </td>
							</tr>
							
							<tr>
							<td><label for="fname">First Name*: </label> </td>
							<td><input id="fname" maxlength="45" name="fname" type="text" /> </td>
							</tr>
							
							<tr>
							<td><label for="lname">Last Name*: </label> </td>
							<td><input id="lname" maxlength="45" name="lname" type="text" /> </td>
							</tr>
							
							<tr>
							<td><label for="email">Email Address*: </label> </td>
							<td><input id="email" maxlength="75" name="email" type="text" style="width:275px;"/></td>
							</tr>

							<tr>
							<td><label for="address">Address Line 1*: </label> </td>
							<td><input id="address" maxlength="50" name="address" type="text" /> </td>
							</tr>
							
							<tr>
							<td><label for="address2">Address Line 2: </label> </td>
							<td><input id="address2" maxlength="50" name="address2" type="text" /> </td>
							</tr>
							
							<tr>
							<td><label for="city">City*: </label> </td>
							<td><input id="city" maxlength="50" name="city" type="text" /> </td>
							</tr>
							
							<tr>
							<td><label for="zip1">Zip Code*: </label> </td>
							<td><input id="zip1" maxlength="50" name="zip1" type="text" style="width:50px"/> - <input id="zip2" maxlength="50" name="zip2" type="text" style="width:50px"/> </td>
							</tr>

							<tr>
							<td><label for="password">Password*:</label></td>
							<td><input id="password" maxlength="45" name="password" type="password" /></td>
							</tr>
							
							<tr>
							<td><label for="role">Role*:</label></td>
							<td><select id="role">
									  <option value="user">User</option>
									  <option value="admin">Admin</option>
								</select></td>
							</tr>
							<tr><td></td>
							</tr>
							<tr>
							<td align="right"><input name="Submit" type="submit" value="Submit" /></td>
							</tr>

							</tbody></table>
                                                
                    </div>
                    <div class="templatemo_post_bottom">
                    
                    	<span class="post">Posted By: SJSU Students</span>
                        <span class="post">Category: <a href="#">Web Design</a></span>
                    </div>
                    
				</div><!-- end of templatemo_post-->
                
            </div><!-- end of left section-->
            
            
        </div><!-- end of container-->
	</div><!-- end of background middle-->
    
     
<div align=center>Created by SJSU Spring 2014 students for CMPE 273 Class Project</div></body>

